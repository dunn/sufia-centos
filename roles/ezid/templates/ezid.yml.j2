default: &default
  host:             {{ ezid_host }}
  port:             {{ ezid_port }}
  user:             <%= Rails.application.secrets.ezid_user %>
  password:         <%= Rails.application.secrets.ezid_pass %>
  ezid_default_shoulder: <%= Rails.application.secrets.ezid_default_shoulder %>

development:
  <<: *default

test:
  <<: *default

production:
  <<: *default
